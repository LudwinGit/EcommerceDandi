
@{
    ViewBag.Title = "Transferencia Monetaria";
   if(Session["CUENTA"]==null || Session["NOMBRE"]==null)
    {
          Response.Redirect("~/Login/vLogin");
    }
}

<h2 align="center">Transferencia Monetaria</h2>
<h2 align="center">Bienvenido(a) @Session["NOMBRE"]</h2>
<h5 align="center">Coloque los datos de la persona a la que le desea realizar una transferencia</h5>

<form method="post">
    <br />
    <table align="center">
        <tr>
            <td>Nombre---- hola jose:</td>
            <td><input type="text" name="nombre" id="txtNombre" /></td>
        </tr>
        <tr>
            <td>
                Apellido:</td>
            <td> <input type="text" name="apellido" id="txtDeporte" /></td>
        </tr>
        <tr>
            <td>
                Numero de cuenta:</td>
            <td> <input type="number" name="edad" id="txtEdad" /> </td>
        </tr>
        <tr><td><button id="aprobar" type="submit">Enviar información</button></td></tr>
            
</table>
</form>
<hr />



